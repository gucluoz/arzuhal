<form name="subjectListForm" action="{% url 'index' %}" method="POST">
  {% csrf_token %}
  <input type="hidden" name="q" id="q" />
  <div class="subjectlist">
    <h4>Konular</h4>
    <ul class="list-unstyled">
      {% for s in subjects %}
      <li><a href="{% url 'indexsearch' s.name %}">{{ s.name }} ({{s.count}})</a></li>
      {% empty %}
      <li>Bo≈ü.</li>
      {% endfor %}
    </ul>
  </div>
</form>